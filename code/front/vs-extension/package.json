{
  "name": "Mline",
  "displayName": "Mline",
  "description": "Analyzes YAML pipeline configuration files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML",
          "yaml"
        ],
        "extensions": [
          ".yaml",
          ".yml"
        ]
      }
    ]
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.75.0"
  },
  "publisher": "Ksenia"
}
